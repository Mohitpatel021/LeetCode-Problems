    "%%time\n",
    "a=[i for i in range(1, 1000000)]\n",
    "n=int(input(\"Enter a number to search: \"))\n",
    "l=0\n",
    "r=len(a)-1\n",
    "while l<=r:\n",
    "    m=(l+r)//2\n",
    "    if a[m]==n:\n",
    "        print(f\"Found {n} at index {m}\")\n",
    "        break\n",
    "    elif a[m]>n:   \n",
    "        r=m-1\n",
    "    else:\n",
    "        l=m+1"
   ]
  }
